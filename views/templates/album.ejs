<!doctype html>
<html>
  <%- include('head', { page : album.name }); %>
  <body>
    <h1>
      artist : <a
        href="/artists/<%= album.artistId %>">
        <%= album.artist.name %>
      </a>
    </h1>
    <h2> album : <%= album.name %> </h2>
    <h4>songs : </h4>
    <ul>
    <% album.songs.forEach(function(song) { %>
      <li> <%= song.name %> </li>
    <% }); %>
    </ul>
    <a href="/album/<%= album._id %>/addsong">add songs</a>
    <a href="/artists/<%= album.artistId %>">back</a>
    <a href="/">main page</a>
  </body>
</html>
